{
  "name": "page-process-list",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "proxy": {
    "/bonita": {
      "target": "http://localhost:8080/bonita",
      "changeOrigin": true,
      "ws": true,
      "pathRewrite": {
        "^/bonita": ""
      }
    },
    "/theme/theme.css": {
      "target": "/theme/theme",
      "changeOrigin": true,
      "ws": true
    }
  },
  "dependencies": {
    "@bonita/api": "file:../common/api",
    "async": "^2.6.0",
    "bootstrap": "^3.3.7",
    "primer": "^10.3.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "npm-run-all -p start-js",
    "build": "npm-run-all build-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "npm-run-all": "^4.1.2"
  }
}

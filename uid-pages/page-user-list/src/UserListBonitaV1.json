  "variables" : {
    "userListCtrl" : {
      "type" : "expression",
      "value" : [
        "return {",
        "    isInformationEmpty(information) {",
        "       if (!information) {",
        "           return \"--\";",
        "       }",
        "       return information;",
        "    },",
        "    getUserStatus(enabled){",
        "        if (enabled) {",
        "            return \"active\";",
        "        }",
        "        return \"inactive\";",
        "    },",
        "    userQueryParameters: function(pageId){",
        "        var usersPerPage = $data.userListData.numberOfUsersPerPage;",
        "        ",
        "        if(pageId === 0){",
        "            //on first call we need load next page",
        "            usersPerPage *= 2;",
        "        }",
        "        var queryParameters = \"c=\" + usersPerPage + \"&p=\" + pageId + \"&time=\" + $data.userListData.timestamp;",
        "        ",
        "        if($data.userListData.userFilters.sortOrder){",
        "            queryParameters += \"&o=\" + $data.userListData.userFilters.sortOrder;",
        "        } else {",
        "            queryParameters += \"&o=lastname+ASC\";",
        "        }",
        "        ",
        "        if($data.userListData.userFilters.searchValue){",
        "            queryParameters += \"&s=\" + $data.userListData.userFilters.searchValue;",
        "        }",
        "    ",
        "        if($data.userListData.userFilters.userStatusEnabled){",
        "            var isEnabled = $data.userListData.userFilters.userStatusEnabled === \"active\" ? true : false; ",
        "            queryParameters += \"&f=enabled=\" + isEnabled;",
        "        }",
        "    ",
        "        return queryParameters;",
        "    },",
        "    getUserDetailsParameters: function(id) {",
        "        return {\"id\": id};",
        "    }",
        "};"
      ],
      "exposed" : false
    },
    "userListData" : {
      "type" : "expression",
      "value" : [
        "return { ",
        "    pageId: 0,",
        "    numberOfUsersPerPage: 10,",
        "    timestamp: 0,",
        "    displayedUsers: [],",
        "    numberOfUsersReturned: 0,",
        "    usersForNextPage: [],",
        "    usersNextResults: undefined,",
        "    sortByOptions: [   ",
        "        { displayKey: \"First name (Asc)\", returnKey: \"firstname+ASC\" },",
        "        { displayKey: \"First name (Desc)\", returnKey: \"firstname+DESC\" },",
        "        { displayKey: \"Last name (Asc)\", returnKey: \"lastname+ASC\" },",
        "        { displayKey: \"Last name (Desc)\", returnKey: \"lastname+DESC\" }",
        "    ],",
        "    userFilters: {",
        "        sortOrder: \"\",",
        "        searchValue: \"\",",
        "        userStatusEnabled: \"active\"",
        "    }",
        "};"
      ],
      "exposed" : false
    },
    "apiUserUrl" : {
      "type" : "constant",
      "value" : [
        "../API/identity/user"
      ],
      "exposed" : false
    },
    "users" : {
      "type" : "url",
      "value" : [
        "{{apiUserUrl}}?{{userListCtrl.userQueryParameters(0)}}"
      ],
      "exposed" : false
    },
    "loadMoreHandler" : {
      "type" : "expression",
      "value" : [
        "//Users",
        "// diplay the first page of users on page load or when the filters change",
        "if($data.users){",
        "    // display half the elements loaded",
        "    $data.userListData.displayedUsers = $data.users.slice(0, $data.userListData.numberOfUsersPerPage);",
        "    // set the next page to be requested to 2 since the first page is in the memory",
        "    $data.userListData.pageId = 2;",
        "    $data.userListData.usersForNextPage = $data.users.slice($data.userListData.numberOfUsersPerPage);",
        "    // saving the other half to be displayed later",
        "    $data.userListData.numberOfUsersReturned = $data.userListData.usersForNextPage.length;",
        "    // set users to undefined as the script is triggered when click on load more users button",
        "    // and to prevent this block of code from being executed (prevent resetting the displayedUsers)",
        "    $data.users = undefined; ",
        "}",
        "",
        "// add next page of users to the list of next users to display",
        "// display the next users from the memory",
        "if($data.userListData.usersNextResults && $data.userListData.usersNextResults.length){",
        "    $data.userListData.displayedUsers = $data.userListData.displayedUsers.concat($data.userListData.usersForNextPage);",
        "    $data.userListData.pageId++;",
        "    $data.userListData.usersForNextPage = $data.userListData.usersNextResults;",
        "    $data.userListData.numberOfUsersReturned = $data.userListData.usersForNextPage.length;",
        "    $data.userListData.usersNextResults = undefined;",
        "}",
        "",
        "// when there are no more users in the memory",
        "if($data.userListData.numberOfUsersReturned > 0 && $data.userListData.usersNextResults && $data.userListData.usersNextResults.length === 0){",
        "     $data.userListData.displayedUsers = $data.userListData.displayedUsers.concat($data.userListData.usersForNextPage);",
        "     $data.userListData.usersForNextPage = undefined;",
        "     $data.userListData.usersNextResults = undefined;",
        "     $data.userListData.numberOfUsersReturned = 0;",
        "}"
      ],
      "exposed" : false
    }
  },

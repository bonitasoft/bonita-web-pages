  "variables" : {
    "tenantStatus" : {
      "type" : "url",
      "value" : [
        "../API/system/tenant/1&t={{response.timestamp}}"
      ],
      "exposed" : false
    },
    "isPaused" : {
      "type" : "expression",
      "value" : [
        "return $data.tenantStatus.paused === 'true';"
      ],
      "exposed" : false
    },
    "messageTenant" : {
      "type" : "expression",
      "value" : [
        "return $data.isPaused \r",
        "        ? uiTranslate(\"Services will resume. Users will be able to log in.\") \r",
        "        : uiTranslate(\"Services will be paused for maintenance.\");"
      ],
      "exposed" : false
    },
    "titleTenant" : {
      "type" : "expression",
      "value" : [
        "return $data.isPaused \r",
        "        ? uiTranslate(\"Are your sure you want to activate the services?\") \r",
        "        : uiTranslate(\"Are your sure you want to deactivate the services?\");"
      ],
      "exposed" : false
    },
    "exp_response" : {
      "type" : "expression",
      "value" : [
        "if(!$data.response.success ){",
        "    $data.response.success = \"none\";",
        "    $data.response.timestamp =  new Date();",
        "}"
      ],
      "exposed" : false
    },
    "session" : {
      "type" : "url",
      "value" : [
        "../API/system/session/unusedId"
      ],
      "exposed" : false
    },
    "response" : {
      "type" : "json",
      "value" : [
        "{\"success\":\"none\", \"timestamp\":0}"
      ],
      "exposed" : false
    },
    "isTechnicalUser" : {
      "type" : "expression",
      "value" : [
        "return $data.session.is_technical_user === 'true';"
      ],
      "exposed" : false
    },
    "sendDataOnClick" : {
      "type" : "expression",
      "value" : [
        "var paused = !($data.isPaused);",
        "return {paused: paused.toString()};"
      ],
      "exposed" : false
    },
    "confirmation" : {
      "type" : "constant",
      "exposed" : false
    },
    "status" : {
      "type" : "expression",
      "value" : [
        "if($data.isPaused){",
        "    return {state: \"Paused\", btnLabel:uiTranslate(\"Resume\"), img: \"paused.jpg\"};",
        "}",
        "return {state: \"Running\", btnLabel:uiTranslate(\"Pause\"), img: \"running.jpg\"};"
      ],
      "exposed" : false
    }
  },

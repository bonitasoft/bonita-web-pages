  "data" : {
    "links" : {
      "type" : "expression",
      "value" : "var htmlElements =  '<ul>' +\r\n                        '<li>' +\r\n                            '<a href=\"https://documentation.bonitasoft.com/bonita/7.9/layouts\" target=\"_blank\" class=\"text-info\">' + uiTranslate('Layout') + '</a>: ' +\r\n                            uiTranslate(\"the frame in which application pages are displayed.\") + \r\n                        '</li>' +\r\n\r\n                        '<li>' +\r\n                            '<a href=\"https://documentation.bonitasoft.com/bonita/7.9/pages\" target=\"_blank\" class=\"text-info\">' + uiTranslate('Pages') + '</a>: ' +\r\n                            uiTranslate('the user interface components of the application. An application must contain at least one page.') + \r\n                         '</li>' +\r\n\r\n                        '<li>' +\r\n                            '<a href=\"https://documentation.bonitasoft.com/bonita/7.5/processes\" target=\"_blank\" class=\"text-info\">' + uiTranslate('Processes') + '</a>: ' +\r\n                            uiTranslate('the logic execution components of the application. An application must contain at least one process.') + ' ' +\r\n                            uiTranslate('A process can contain') + ' ' + '<a href=\"https://documentation.bonitasoft.com/bonita/7.5/forms\" target=\"_blank\" class=\"text-info\">' + uiTranslate('forms') + '</a>, ' +\r\n                            uiTranslate('which are the user interface of the process.') +\r\n                        '</li>' +\r\n\r\n                        '<li>' +\r\n                            '<a href=\"https://documentation.bonitasoft.com/bonita/7.5/rest-api-extensions\" target=\"_blank\" class=\"text-info\">' + uiTranslate('REST API extensions') + '</a>: ' +\r\n                            uiTranslate('called from pages or forms to retrieve data to be presented to the user.') +\r\n                        '</li>' +\r\n                        \r\n                        '<li>' +\r\n                            '<a href=\"https://documentation.bonitasoft.com/bonita/7.5/themes\" target=\"_blank\" class=\"text-info\">' + uiTranslate('Theme') + '</a> : ' +\r\n                            uiTranslate('the appearance of application pages, including fonts and colors.') +\r\n                        '</li>' +\r\n                    '</ul>';\r\n\r\nreturn htmlElements;",
      "exposed" : false
    },
    "newApplication" : {
      "type" : "expression",
      "value" : "var text = '<p> ' +\r\n                uiTranslate('Since Bonita 7.5.0, the definition of applications should be done from the studio, which makes it more straightforward to develop and package with other application artifacts.') + ' ' +\r\n                uiTranslate('For Enterprise, Efficiency and Performance editions, a graphical editor is provided on top of xml source code.') + ' ' +\r\n                uiTranslate('For more details, go to') + ' ' +\r\n                '<a href=\"https://documentation.bonitasoft.com/bonita/7.9/applicationCreation\" target=\"_blank\" class=\"text-info\"> ' +\r\n                    uiTranslate('Application descriptors') + \r\n                '</a>.'  + \r\n            '</p>';\r\n\r\nreturn text;",
      "exposed" : false
    }

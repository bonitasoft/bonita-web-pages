{
  "extends": [
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "babelOptions": {
      "presets": ["@babel/preset-react"]
   }
  },
  "rules": {
    "semi": 2,
    "prettier/prettier": ["error", {"singleQuote": true, "endOfLine":"auto"}]
  }
}